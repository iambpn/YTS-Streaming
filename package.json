{
  "name": "YTS-Streaming",
  "version": "2.2.0",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "electron-is-dev": "^2.0.0",
    "electron-window-state": "^5.0.3",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sass": "^1.34.1",
    "srt2vtt": "^1.3.1",
    "typescript": "^4.3.2",
    "webtorrent": "^1.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch-tsc-public": "tsc --watch --project public",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run watch-tsc-public\" \"npm start\" \"wait-on http://localhost:3000 && npm run electron\"",
    "electron-pack": "electron-builder build",
    "preelectron-pack": "npm run build"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "appId": "com.mbpn1.yts-streaming",
    "copyright": "Copyright 2020 Maharjan-Bipin",
    "compression": "normal",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false,
      "deleteAppDataOnUninstall": true,
      "allowElevation": true,
      "include": "assets/installerMacro.nsh",
      "warningsAsErrors": false
    }
  },
  "homepage": "./",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.7",
    "concurrently": "^6.2.0",
    "electron": "^13.6.6",
    "electron-builder": "^22.10.5",
    "wait-on": "^5.3.0",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.15",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/webtorrent": "^0.109.0"
  }
}
